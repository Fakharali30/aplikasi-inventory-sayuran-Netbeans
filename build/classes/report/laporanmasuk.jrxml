	<parameter name="NTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     transaksimasuk.`idtransaksi` AS transaksimasuk_idtransaksi,
     transaksimasuk.`idbarang` AS transaksimasuk_idbarang,
     transaksimasuk.`namapetugas` AS transaksimasuk_namapetugas,
     transaksimasuk.`namasp` AS transaksimasuk_namasp,
     transaksimasuk.`tanggal` AS transaksimasuk_tanggal,
     transaksimasuk.`jumlah` AS transaksimasuk_jumlah
FROM
     `transaksimasuk` transaksimasuk

WHERE
    idtransaksi LIKE '$P!{NTA}%' OR
     tanggal LIKE '%$P!{NTA}%'
ORDER BY
idtransaksi ASC]]>
	</queryString>
